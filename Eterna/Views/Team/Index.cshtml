
<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
        <div class="container">

            <ol>
                <li><a href="index.html">Home</a></li>
                <li>Team</li>
            </ol>
            <h2>Team</h2>

        </div>
    </section><!-- End Breadcrumbs -->
    <!-- ======= Team Section ======= -->
    <section id="team" class="team">
        <div class="container">

            <div class="row">

                @foreach (Team team in Model)
                {
                <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
                    <div class="member">
                        <img src="@team.Image" alt="">
                        <h4>@team.Name @team.SurName</h4>
                        <span>@team.Position</span>
                        <p>
                            @team.Description
                        </p>
                        <div class="social">

                            @if (team.FacebookUrl != null)
                            {
                                <a href="@team.FacebookUrl"><i class="bi bi-facebook"></i></a>}

                            @if (team.TwitterUrl != null)
                            {
                                <a href="@team.TwitterUrl"><i class="bi bi-twitter"></i></a>}

                            @if (team.InstagramUrl != null)
                            {
                                <a href="@team.InstagramUrl"><i class="bi bi-instagram"></i></a>}

                            @if (team.LinkedinUrl != null)
                            {
                                <a href="@team.LinkedinUrl"><i class="bi bi-linkedin"></i></a>}

                        </div>
                    </div>
                </div>
                        }
               



                </div>

        </div>
    </section><!-- End Team Section -->

</main><!-- End #main -->